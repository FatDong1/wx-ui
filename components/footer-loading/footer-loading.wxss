/* components/footerLoading/footerLoading.wxss */
.footerTips{
    clear: both;
    width: 100%;
    height: 64rpx;
    border-top: 1rpx solid #f3f3f3;
    font-size: 28rpx;
    color: #999;
    display: flex;
    justify-content: center;
    align-items: center;
}

.loadingIcon{
    width:32rpx;
    height:32rpx;
    margin-right: 15rpx;
}

.isLoadOver{
    width: 100%;
    height: 64rpx;
    border-top: 1rpx solid #f3f3f3;
    font-size: 28rpx;
    color: #999;
    display: flex;
    justify-content: center;
    align-items: center;
}